<form action="/egg-timer/calculator/interval/calculate" method="GET" id="calculator">
	
	<fieldset>
		<legend>Choose a start date</legend>
			
			<p><input type="date" name="start-date" id="start-date" required>
		
	</fieldset>
	
	<fieldset>
		<legend>Choose an end date</legend>
			
			<p><input type="date" name="end-date" id="end-date" required>
		
	</fieldset>
	
	<button type="submit" id="calculate">Calculate</button>
</form>